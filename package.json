{
  "name": "blog",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "6.7.0"
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha test",
    "build": "webpack --watch",
    "dev": "webpack-dev-server --inline --hot",
    "build-css": "node-sass --include-path scss resources/assets/sass/main.scss dist/css/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "start:dev": "concurrently \"npm run watch-css\"  \"npm run build\"",
    "start:prod": "concurrently \"npm run build-css\"  \"npm run build\"",
    "start": "node index.js",
    "postinstall": "npm run start:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrunoCasotto/blog.git"
  },
  "_moduleAliases": {
    "@app": ".",
    "@route": "src/routes",
    "@modules": "src/modules",
    "@config": "config",
    "@helpers": "src/helpers",
    "@resources": "resources",
    "@node": "node_modules"
  },
  "keywords": [
    "blog"
  ],
  "author": "Bruno Casotto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BrunoCasotto/blog/issues"
  },
  "homepage": "https://github.com/BrunoCasotto/blog#readme",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "handlebars": "^4.0.6",
    "hapi": "^16.0.1",
    "hapi-plugin-co": "^0.9.4",
    "module-alias": "^2.0.0",
    "node-sass": "^3.13.0",
    "nunjucks-hapi": "^2.1.0",
    "path": "^0.12.7",
    "request": "^2.79.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "concurrently": "^2.2.0",
    "es6-promise": "^3.2.1",
    "firebase": "^3.6.2",
    "growl-alert": "^0.3.2",
    "inert": "^4.0.3",
